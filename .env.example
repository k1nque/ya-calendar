# Copy to .env and fill
POSTGRES_DB=ya_calendar
POSTGRES_USER=ya_user
POSTGRES_PASSWORD=ya_pass
DATABASE_URL=postgresql+psycopg2://ya_user:ya_pass@postgres:5432/ya_calendar
REDIS_URL=redis://redis:6379/0
# Telegram bot token and admin id
TG_BOT_TOKEN=your_telegram_bot_token_here
ADMIN_TELEGRAM_ID=123456789
# CalDAV credentials
CALDAV_EMAIL=your@calendar.email
CALDAV_PASSWORD=your_calendar_password
CALDAV_WEBSITE=https://caldav.yandex.ru/
# Worker poll interval (seconds)
WORKER_POLL_SECONDS=600
